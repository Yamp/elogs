http://elogs.club {
    redir {
        if {scheme} is http
        / https://{host}{uri}
    }
}

https://elogs.club/vueapp {
    root /srv/frontend/dist
    errors {
	404 index.html
    }  
}

https://elogs.club {
    root /srv
    proxy / 127.0.0.1:8002 {
        transparent
    }    
    proxy /e-logs 127.0.0.1:8001 {
        websocket
    }
}

